        <div n="tablet" type="textpart">
          <div n="obverse" type="textpart">
            <l n="1">{d}nisaba nam-lugal-bi</l>
            <l n="2">suhuš-bi gin6-ne-me-en</l>
            <l n="3">nam-en-na</l>
            <l n="4">⸢aga⸣ gar-gar-ra-me-en</l>
            <l n="5">u4 ussu-kam</l>
          </div>
          <div n="reverse" type="textpart">
            <l n="1">šu-zu! im-ma</l>
            <l n="2">si ba-ni-sa2</l>
            <l n="3">ša3 ta-ba-kam</l>
            <l n="4">gu3-šum2 i3-sa6-sa6</l>
