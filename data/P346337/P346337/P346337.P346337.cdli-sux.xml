      <div n="urn:cts:cdli:P346337.P346337.cdli-sux" type="edition" xml:lang="sux">
        <div n="tablet" type="textpart">
          <div n="obverse" type="textpart">
            <l n="1">[ku3] in-[tuku]</l>
            <l n="2">{na4#}za-gin3 in-⸢tuku⸣</l>
            <l n="3">⸢gu4⸣ in-tuku</l>
            <l n="4">[udu] in-tuku</l>
            <l n="5">⸢ka2⸣ lu2 še in-tuku</l>
            <l n="6">u4#? mi-ni-ib-zal-zal-e</l>
          </div>
          <div n="reverse" type="textpart"/>
        </div>
      </div>
      <div type="translation">
        <div n="tablet" type="textpart"/>
      </div>
      <div type="translation" xml:lang="en">
        <l n="1">The one who has possessed silver</l>
        <l n="2">The one who has possessed lapis</l>
        <l n="3">The one who has possessed oxen</l>
        <l n="4">The one who has possessed sheep</l>
        <l n="5">(At) the gate (of) the one who has possessed barley</l>
        <l n="6">Will spent the day there</l>
      </div>
